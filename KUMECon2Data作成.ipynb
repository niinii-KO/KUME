def num_maker(CorA,Age,cri,tb):
  critb = cri[CorA]
  lislis = []
  if CorA == 0:
    if Age > 1 and Age<13:
      l = [0,2,3,4,5]
    else:
      l = [1,2,3,4,5]
    
    if Age == 0:
      l = l + [6]
    elif Age < 12:
      l = l + [7]
    else:
      l = l + [8]
  else:
    l = [0,1,2 + rd.choice(2),4 + rd.choice(2),6,7]
  
  j=0
  for i in l:
    #print(i)
    if critb[i] ==[8]:
      a = rd.choice(3) + 1
      a = 10**tb[j] * a
    elif len(critb[i]) == 3:
      a = critb[i][tb[i]]
    else:
      #print(critb[i])
      #print(tb)
      a = round(random.uniform(critb[i][tb[j]], critb[i][tb[j]+1]),2)
    lislis = lislis + [a]
    j=j+1
  return l + lislis

def num_maker2(f, mu, sigma,roundn=2):
  TF = False
